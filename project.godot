; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[android]

modules="org/godotengine/godot/GodotPlayGameServices"

[application]

config/name="Multiplayer Shooter"
run/main_scene="res://interface/Menu.tscn"
config/icon="res://icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

Network="*res://Network.gd"

[debug]

settings/fps/force_fps=48

[display]

window/size/width=960
window/size/height=540
window/size/resizable=false
window/size/always_on_top=true
window/vsync/use_vsync=false
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
